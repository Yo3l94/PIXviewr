{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"target": "es5",
		"lib": [
			"es2015",
			"dom"
		],
		"jsx": "react",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"skipLibCheck": true,
		"esModuleInterop": true
	}
}